<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<title></title>
	</head>
	<body>
		<script>
			
			//打印99乘法表
			/*  1*1=1 
				1*2=2 2*2=4
				1*3=3 2*3=6 3*3=9
								…… 9*9=81 
			*/
			
			for(var i=1 ; i<10 ; i++){
				// document.write(i + "&nbsp")
				for(var j=1 ; j<=i ;j++){
					document.write("<span>" + i + "*" + j + "=" + i*j + "&nbsp;&nbsp;&nbsp;" + "</span>" )
				}
				document.write("<br />")
			}
			
		</script>
	</body>
</html>
