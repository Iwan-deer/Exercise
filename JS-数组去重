			var arr1 = [1,2,3,4,2,5,1,6,7,4,2]
			//数组去重
			for(i=0 ; i<arr.length ; i++){
				for(j=i+1 ; j<arr.length ; j++){
					if(arr[i] == arr[j]){
						arr.splice(j,1);
						//以防元素连续重复时无法检测
						j--;
					}
				}
			}
			console.log(arr)
		</script>
